  "dependencies": {
    "chokidar": "^2.0.4",
    "commonmark": "^0.28.1",
    "express": "^4.16.4",
    "express-mustache-jwt-signin": "^0.3.2",
    "express-mustache-overlays": "^0.3.2",
    "isomorphic-fetch": "^2.2.1",
    "yaml-front-matter": "^4.0.0"
  },
  "devDependencies": {
    "standard": "^12.0.1"
  }
