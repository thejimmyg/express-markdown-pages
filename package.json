{
  "name": "express-markdown-pages",
  "version": "0.1.1",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "docker:build": "docker build . -t thejimmyg/express-markdown-pages:0.1.1",
    "docker:push": "docker push thejimmyg/express-markdown-pages:0.1.1",
    "docker:build:test": "docker build . -t thejimmyg/express-markdown-pages:test",
    "docker:push:test": "docker push thejimmyg/express-markdown-pages:test",
    "docker:run:local": "docker-compose stop && docker-compose pull && docker-compose up --build -d",
    "docker:stop:local": "docker-compose stop",
    "docker:logs:local": "docker-compose logs --tail='all' -f",
    "start": "node bin/server.js",
    "fix": "standard --fix"
  },
